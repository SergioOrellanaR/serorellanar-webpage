{{#if usedTechnologies}}
<section class="site-section" id="section-usedTechnologies">
    <div class="container">
        <div class="row">
            <div class="section-heading text-center col-md-12">
                <h2>for its development <strong>it was used</strong></h2>
            </div>
        </div>
        <div class="container">
            <div class="row">
                {{#each usedTechnologies}}
                <div class="col-sm col-sm text-center mb-5">
                    <span class="icon">
                        <span><img src={{this.icon}} /></span>
                    </span>
                    <h6 class="mb-6">{{this.name}}</h6>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</section>
{{/if}}